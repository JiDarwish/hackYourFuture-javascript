mkdir HW-4.1
touch blank

for i in {1..5}; do echo "Hello" >> greetings.txt;done
for x in {1..3}; do cp greetings.txt $x.txt;done

echo "cat" >> pets.txt
echo "dog" >> pets.txt
echo "hamster" >> pets.txt

echo "cat" >> commands.txt
echo "ls" >> commands.txt
echo "pwd" >> commands.txt

grep -v -f pets.txt commands.txt >> lovelyCommands.txt
grep -v -f commands.txt pets.txt >> loveltCommands.txt
